      "required": true
    },
    {
      "name": "name",
      "type": "string",
      "description": "Model name in registry",
      "required": true
    },
    {
      "name": "description",
      "type": "string",
      "description": "Model description",
      "required": false
    }
  ],
  "dependencies": [
    "Dockerfile_solar"
  ]
}